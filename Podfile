	pod 'SwiftCommons', :git => 'https://github.com/mdermejian/SwiftCommons.git'
	pod 'Commons', :git => 'https://github.com/mdermejian/Commons.git'
	pod 'DateTools', '~> 1.7'


  target 'ProjectsTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
